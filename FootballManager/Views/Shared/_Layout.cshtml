<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Basic -->
    <meta charset="utf-8">
    <title>@ViewBag.Title - Football Manager by PSW</title>
    <meta name="keywords" content="football,manager,soccer,team,championship,journey,match,season,country,matchGoal" />
    <meta name="description" content="Football Manager">
    <meta name="author" content="Grupo 2">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Theme CSS -->
    <link href="@Styles.Url("~/Content/Theme/css/style.css")" rel="stylesheet" media="screen" />
    <!-- Responsive CSS -->
    <link href="@Styles.Url("~/Content/Theme/css/theme-responsive.css")" rel="stylesheet" media="screen">
    <!-- Skins Theme -->
    <link href="@Styles.Url("~/Content/Theme/css/green.css")" rel="stylesheet" media="screen" class="skin">

    <!-- Favicons -->
    <link rel="shortcut icon" href="@Url.Content("~/favicon.png")">
    <link rel="apple-touch-icon" href="@Url.Content("~/favicon.png")">

    <!-- Plugins Styles -->
    @Styles.Render("~/Content/datepicker.css")
    @Styles.Render("~/Content/select2.css")
    @Styles.Render("~/Content/select2-bootstrap.css")

    <!-- Head Libs -->
    @Scripts.Render("~/bundles/modernizr")

    <!--[if IE]>
        <link rel="stylesheet" href="css/ie/ie.css">
    <![endif]-->
    <!--[if lte IE 8]>
        <script src="js/responsive/html5shiv.js"></script>
        <script src="js/responsive/respond.js"></script>
    <![endif]-->
    <!-- Skins Changer-->
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    

</head>
<body>
    <!-- layout-->
    <div id="layout" class="layout-boxed">
        <!-- Header-->
        <header>
            <!-- headerbox-->
            <div class="headerbox">
                <div class="container">
                    <div class="row">
                        <!-- Logo-->
                        <div class="col-md-3 logo">
                            <a href="@Url.Action("Index", "Home")">
                                <img src="@Url.Content("~/Content/Theme/img/FM_LOGO.png")" />
                            </a>
                        </div>
                        <!-- End Logo-->
                    </div>
                </div>
            </div>
            <!-- End headerbox-->
            <!-- mainmenu-->
            <nav class="mainmenu">
                <div class="container">
                    <!-- Menu-->
                    <ul class="sf-menu" id="menu">
                        <li class="selected">
                        @Html.ActionLink("Home", "Index", "Home")
                        </li>
                        <li class="current">
                            <a href="#">Teams</a>
                            <ul class="sub-current">
                                <li>
                                   @Html.ActionLink("View Teams", "Index", "Teams")
                                </li>
                                <li>
                                    <a href="create_team.html">Create Team</a>
                                </li>
                                <li>
                                    <a href="edit_team.html">Edit Team</a>
                                </li>
                                <li>
                                    <a href="remove_team.html">Remove Team</a>
                                </li>
                            </ul>

                        </li>
                        <li class="current">
                            <a href="championships.html">Championships</a>
                            <ul class="sub-current">
                                <li>
                                    <a href="championship.html">View Championship</a>
                                </li>
                                <li>
                                    <a href="create_championship.html">Create Championship</a>
                                </li>
                                <li>
                                    <a href="edit_championship.html">Edit Championship</a>
                                </li>
                                <li>
                                    <a href="remove_championship.html">Remove Championship</a>
                                </li>
                            </ul>

                        </li>
                        <li class="current">
                            <a href="matches.html">Matches</a>
                            <ul class="sub-current">
                                <li>
                                    <a href="matches.html">View Matches</a>
                                </li>
                                <li>
                                    <a href="create_matches.html">Create Matches</a>
                                </li>
                                <li>
                                    <a href="edit_matches.html">Edit Matches</a>
                                </li>
                                <li>
                                    <a href="remove_matches.html">Remove Matches</a>
                                </li>
                            </ul>

                        </li>

                        <li>
                            <a href="results.html">Results</a>
                        </li>
                    </ul>
                    <!-- End Menu-->
                </div>
            </nav>
            <!-- End mainmenu-->
        </header>
        <!-- End Header-->
