                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        @Html.ActionLink("FManager", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                    </div>
                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("Championships", "Index", "Championships")</li>
                            <li>@Html.ActionLink("Seasons", "Index", "Seasons")</li>
                            <li>@Html.ActionLink("Matches", "Index", "Matches")</li>
                            <li>@Html.ActionLink("Teams", "Index", "Teams")</li>
                            <li>@Html.ActionLink("Players", "Index", "Players")</li>
                        </ul>
                        @Html.Partial("_LoginPartial")
                    </div>
                </div>
            </div>*@

        @RenderBody()
        <!-- footer-->
        <footer id="footer">
            <div class="container">

            </div>
        </footer>
        <!-- End footer-->
        <!-- footer Down-->
        <div class="footer-down">
            <div class="container">
                <div class="row">
                    <div class="col-md-5">
                        <p>&copy; 2014 Football Manager . All Rights Reserved.  2014</p>
                    </div>
                    <div class="col-md-7">
                        <!-- Nav Footer-->
                        <ul class="nav-footer">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("Championships", "Index", "Championships")</li>
                            <li>@Html.ActionLink("Seasons", "Index", "Seasons")</li>
                            <li>@Html.ActionLink("Matches", "Index", "Matches")</li>
                            <li>@Html.ActionLink("Teams", "Index", "Teams")</li>
                            <li>@Html.ActionLink("Players", "Index", "Players")</li>               
                        </ul>
                        <!-- End Nav Footer-->
                    </div>
                </div>
            </div>
        </div>
        <!-- footer Down-->

    </div>
    <!-- End layout-->
    <!-- ======================= JQuery libs =========================== -->
    <!-- jQuery local-->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Content/Theme/js/jquery.js")
    <!--Nav-->
    @Scripts.Render("~/Content/Theme/js/tinynav.js")
    @Scripts.Render("~/Content/Theme/js/hoverIntent.js")
    @Scripts.Render("~/Content/Theme/js/superfish.js")
    @Scripts.Render("~/Content/Theme/js/jquery.sticky.js")
    <!--Totop-->
    @Scripts.Render("~/Content/Theme/js/jquery.ui.totop.js")
    <!--Slide-->
    @Scripts.Render("~/Content/Theme/js/camera.js")
    @Scripts.Render("~/Content/Theme/js/jquery.easing.1.3.min.js")
    <!-- Maps -->
    @Scripts.Render("~/Content/Theme/js/gmap3.js")
    <!--Ligbox-->
    @Scripts.Render("~/Content/Theme/js/jquery.fancybox.js")
    <!-- carousel.js-->
    @Scripts.Render("~/Content/Theme/js/carousel.js")
    <!-- Filter -->
    @Scripts.Render("~/Content/Theme/js/jquery.isotope.js")
    <!-- Twitter Feed-->
    @Scripts.Render("~/Content/Theme/js/jquery.tweet.js")
    <!-- flickr Feed-->
    @Scripts.Render("~/Content/Theme/js/jflickrfeed.min.js")
    <!-- Counter -->
    @Scripts.Render("~/Content/Theme/js/jquery.countdown.js")
    <!--Theme Options-->
    @Scripts.Render("~/Content/Theme/js/theme-options.js")
    @Scripts.Render("~/Content/Theme/js/jquery.cookies.js")
    <!-- Bootstrap.js-->
    @Scripts.Render("~/Content/Theme/js/bootstrap.js")
    <!--MAIN FUNCTIONS-->
    @Scripts.Render("~/Content/Theme/js/main.js")
    <!-- ======================= End JQuery libs =========================== -->

        @RenderSection("scripts", required: false)
</body>
</html>
